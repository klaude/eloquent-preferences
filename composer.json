        "laravel",
        "eloquent",
        "preferences"
    ],
    "homepage": "https://github.com/klaude/eloquent-preferences",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Laude",
            "email": "nerfyoda@gmail.com"
        }
    ],
    "require": {
